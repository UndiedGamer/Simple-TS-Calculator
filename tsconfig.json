{
    "compilerOptions": {
      
      "target": "es2021",
      "module": "ESNext",
      "checkJs": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "skipLibCheck": false,
      "forceConsistentCasingInFileNames": true
    }
  }